<script>

    $(document).ready(function () {
        $.get("api/TeamMember", function(data){
            var html = '';

            for(var i = 0; i < data.length; i++) {
                html += '<div class="col-lg-3 col-md-6 d-flex align-items-stretch"> <div class="member" data-aos="fade-up">' +
                '<div class="member-img"><img src="/' + data[i].image + '" class="img-fluid" alt=""></div>' +
                '<div class="member-info"><h4>' + data[i].name + '</h4><span><i class="bi bi-geo-alt"></i> ' + data[i].location + 
                '</span><span><b><i class="bi bi-bar-chart"></i> ' + data[i].rankName + '</b></span> <a class="btn btn-outline-dark mt-2" href="TeamMember?id=' + data[i].id + '">Details</a>' +
                '</div></div></div>';
            }
            $('.teamdiv').html(html);
        });

    
        $.get("api/HowTo", function(data){

            for(var i = 0; i < data.length; i++) {
                var html = '<div class="col-lg-4 col-md-6"> <div class="w-100 shadow rounded p-3"> <div class="text-center font-weight-bold my-2">' +
                         data[i].title + '<hr /></div><div id="howto' + i + '" class="text-center"></div></div></div>';

                $('.howtodiv').append(html);

                if(data[i].file.endsWith("png") || data[i].file.endsWith("jpg") || data[i].file.endsWith("jpeg") ){
                    $('#howto' + i ).html('<img src="/' + data[i].file + '" class="howtofile">');
                }
                else{
                    $('#howto' + i ).html('<video src="/' + data[i].file + '" controls="show"  class="howtofile"></video>');
                }
            }
        });
    });
</script>